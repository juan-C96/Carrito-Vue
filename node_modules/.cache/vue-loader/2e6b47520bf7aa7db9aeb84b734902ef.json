AqCA","file":"Producto.vue","sourceRoot":"src/components","sourcesContent":["<template>\r\n    <div class=\"col-12 col-md-6\">\r\n      <div class=\"item shadow mb-4\">\r\n        <img :src=\"producto.imagen\" class=\"item-image\">\r\n        <div>\r\n          <h5 class=\"item-title\">{{producto.titulo}}</h5>\r\n          <p class=\"item-codigo\">Código: {{producto.codigo}}</p>\r\n        </div>\r\n        <div class=\"item-details\">\r\n          <p class=\"description-p\"><b>Descripción: </b>{{producto.descripcion}}</p>\r\n          <p>Stock: <p class=\"item-stock\" id=\"text1\">{{producto.stock}}</p>\r\n        </div>\r\n        <div class=\"item-details\">\r\n          <h4 class=\"item-price\">${{Number(producto.precio).toFixed()}}</h4>\r\n          <button class=\"item-button btn btn-primary addToCart\" data-id=\"1\">AÑADIR AL\r\n            CARRITO</button>\r\n          <button @click=\"agregarCarro(producto)\" class=\"btn btn-primary form-control\">\r\n            {{estaEnCarrito ? 'Agregado' : 'Agregar al Carrito' }}\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n\r\n<script>\r\n    export default {\r\n        name: 'Producto',\r\n        props: ['producto', 'estaEnCarrito'],\r\n        methods:{\r\n            agregarCarro(producto){\r\n                this.$emit('agregar-carro', producto);\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style >\r\n@import '../assets/style.css';\r\n</style>"]}]}